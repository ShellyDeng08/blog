<nav id="mobile-nav">
  <% for (var i in theme.menu){ %>
    <a href="<%- url_for(theme.menu[i]) %>" class="mobile-nav-link"><%= i %></a>
  <% } %>
  <% site.categories.each(function(category) { %>
    <a class="mobile-nav-link" href="<%- url_for(category.path) %>" title="<%= category.name %>">
        <%= category.name %>
    </a>
  <% }); %>
</nav>